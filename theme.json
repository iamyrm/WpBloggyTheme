{
  "$schema": "https://schemas.wp.org/wp/6.5/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultPalette": false,
      "palette": [
        {
          "slug": "b-white",
          "color": "rgb(255,255,255)",
          "name": "Bloggy Whitw"
        },
        {
          "slug": "b-gray-100",
          "color": "rgb(243 244 246)",
          "name": "Bloggy Gray 100"
        },
        {
          "slug": "b-gray-200",
          "color": "rgb(229 231 235)",
          "name": "Bloggy Gray 200"
        },
        {
          "slug": "b-gray-300",
          "color": "rgb(209 213 219)",
          "name": "Bloggy Gray 300"
        },
        {
          "slug": "b-gray-400",
          "color": "rgb(156 163 175)",
          "name": "Bloggy Gray 400"
        },
        {
          "slug": "b-gray-500",
          "color": "rgb(107 114 128)",
          "name": "Bloggy Gray 500"
        },
        {
          "slug": "b-gray-600",
          "color": "rgb(75 85 99)",
          "name": "Bloggy Gray 600"
        },
        {
          "slug": "b-gray-700",
          "color": "rgb(55 65 81)",
          "name": "Bloggy Gray 700"
        },
        {
          "slug": "b-gray-800",
          "color": "rgb(31 41 55)",
          "name": "Bloggy Gray 800"
        },
        {
          "slug": "b-gray-900",
          "color": "rgb(17 24 39)",
          "name": "Bloggy Gray 900"
        },
        {
          "slug": "b-primary",
          "color": "rgb(239 68 68)",
          "name": "Bloggy Primary"
        },
        {
          "slug": "b-accent",
          "color": "rgb(67 56 202)",
          "name": "Bloggy Accent"
        },
        {
          "slug": "b-success",
          "color": "rgb(52 211 153)",
          "name": "Bloggy Success"
        },
        {
          "slug": "b-info",
          "color": "rgb(96 165 250)",
          "name": "Bloggy Info"
        },
        {
          "slug": "b-warning",
          "color": "rgb(251 146 60)",
          "name": "Bloggy Warning"
        },
        {
          "slug": "b-danger",
          "color": "rgb(236 72 153)",
          "name": "Bloggy Danger"
        }
      ],
      "background": true,
      "text": true,
      "link": true,
      "custom": true,
      "defaultGradients": false,
      "gradients": [
        {
          "slug": "bloggy-summer",
          "name": "Bloggy Summer",
          "gradient": "linear-gradient(#a8ff78,#78ffd6)"
        }
      ]
    },
    "blocks": {
      "core/site-title": {
        "color": {
          "defaultPalette": true,
          "palette": [
            {
              "slug": "b-primary",
              "color": "#ef4444",
              "name": "Bloggy Primary"
            }
          ],
          "link": true,
          "custom": false,
          "gradients": [],
          "customGradient": false
        },
        "typography": {
          "fontFamilies": [
            {
              "fontFamily": "Pacifico, sans-serif",
              "slug": "b-pacifico",
              "name": "Bloggy Pacifico"
            }
          ],
          "fontSizes": [
            {
              "slug": "small",
              "size": "0.75rem",
              "name": "Small"
            },
            {
              "slug": "medium",
              "size": "1.25rem",
              "name": "Medium"
            },
            {
              "slug": "large",
              "size": "2.25rem",
              "name": "Large"
            },
            {
              "slug": "x-large",
              "size": "3rem",
              "name": "Extra Large"
            }
          ],
          "lineHeight": true,
          "dropCap": true,
          "fontWeight": true,
          "fontStyle": true,
          "textTransform": true,
          "textDecoration": true
        }
      },
      "core/preformatted": {
        "typography": {
          "fontSizes": [],
          "customFontSize": false
        }
      }
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Rubik, sans-serif",
          "slug": "b-rubik",
          "name": "Bloggy Rubik"
        }
      ]
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1100px"
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "units": ["px", "rem", "vh", "vw", "%"],
      "blockGap": true
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--b-gray-700)",
      "background": "var(--wp--preset--color--b-white)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--b-primary)"
        }
      }
    },
    "blocks": {
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--b-pacifico)"
        }
      },
      "core/button": {
        "color": {
          "gradient": "var(--wp--preset--gradient--bloggy-summer)"
        }
      },
      "core/pullquote": {
        "border": {
          "width": "2px",
          "radius": "10px",
          "style": "solid",
          "color": "var"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--b-rubik)",
      "fontSize": "16px",
      "fontStyle": "normal",
      "fontWeight": "normal",
      "lineHeight": "inherit",
      "textDecoration": "none",
      "textTransform": "none"
    },
    "spacing": {
      "margin": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "padding": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "blockGap": "4rem"
    }
  },
  "customTemplates": [
    {
      "name": "full-width-page",
      "title": "Full Width Page",
      "postTypes": ["page"]
    }
  ]
}
